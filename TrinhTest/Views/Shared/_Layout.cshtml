    <style>
        .pulse {
            height: 50px;
            width: 50px;
            position: absolute;
            margin: auto;
            display: grid;
            place-items: center;
        }

            .pulse:before,
            .pulse:after {
                content: "";
                position: absolute;
                height: 100%;
                width: 100%;
                background-color: #8a82fb;
                border-radius: 50%;
                z-index: -1;
                opacity: 0.7;
            }

            .pulse:before {
                animation: pulse 5s ease-out infinite;
            }

            .pulse:after {
                animation: pulse 5s 1s ease-out infinite;
            }
    </style>
