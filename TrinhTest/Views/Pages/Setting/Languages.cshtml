@{
    Layout = null;
}
<script type="text/javascript">
    $(document).ready(function () {
        document.getElementById("Languagesdropdown").addEventListener('click', function () {
            if (document.getElementById("CkeckLanguages").hasClass('show')) {
                document.getElementById("CkeckLanguages").removeClass('show');
            }
            else {
                document.getElementById("CkeckLanguages").addClass('show');
            }
        });


    }
</script>
